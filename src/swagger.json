                      "application/json": {
                          "schema": {
                              "type": "object",
                              "properties": {
                                  "name": {
                                      "type": "string"
                                  },
                                  "email": {
                                      "type": "string"
                                  }
                              },
                              "example": {
                                  "name": "User name sample",
                                  "email": "User email sample"
                              }
                          }
                      }
                  }
              },
              "responses": {
                  "201": {
                      "description": "Created"
                  },
                  "400": {
                      "description": "Email already exists"
                  }
              }
          }
      }
