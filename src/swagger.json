          "get": {
            "tags": ["Users"],
            "summary": "List all users",
            "responses": {
              "200": {
                  "description": "OK",
                  "content": {
                      "application/json": {
                          "schema": {
                              "type": "array",
                              "items": {
                                  "type": "object",
                                  "properties": {
                                      "name": {
                                          "type": "string"
                                      },
                                      "email": {
                                          "type": "string"
                                      }
                                  }
                              }
